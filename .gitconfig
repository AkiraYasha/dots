# local/privat config goes in the include
[include]
  path = ~/.gitconfig.local

[hub]
  protocol = https

[alias]
  co = checkout

[color]
  diff = auto
  status = auto
  branch = auto
  ui = true

[core]
  excludesfile = ~/.gitignore
  editor = nano

[apply]
  whitespace = nowarn

[mergetool]
  keepBackup = false

[difftool]
  prompt = false

[help]
  autocorrect = 1

[push]
	default = simple

[merge]
	tool = vscode

[mergetool "vscode"]
	cmd = "code --wait "

[url "https://github.com/"]
        insteadOf = github:
